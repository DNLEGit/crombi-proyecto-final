generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  userId   String @id @unique() @default(uuid())
  name     String
  email    String @unique
  password String
  role     Role   @default(USER)
}

enum Role {
  ADMIN
  USER
}
